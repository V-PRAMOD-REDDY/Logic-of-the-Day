   "source": [
    "#Day-1\n",
    "n=int(input(\"Enter the digits to sum:\"))\n",
    "sum=0\n",
    "while n>0:\n",
    "    reminder=n%10 \n",
    "    sum=sum+reminder\n",
    "    n=n//10\n",
    "print(\"Sum of digits is:\",sum)"
